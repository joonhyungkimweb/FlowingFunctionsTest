{
  "name": "nocodingflow",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "lint:fix": "eslint ./src --ext .jsx,.js,.ts,.tsx --quiet --fix --ignore-path ./.gitignore",
    "lint:format": "prettier  --loglevel warn --write \"./**/*.{js,jsx,ts,tsx,md,json}\" ",
    "sass": "node-sass src/styles/scss -o src/styles/css",
    "sass:watch": "node-sass src/styles/scss -o src/styles/css --watch",
    "type-check": "tsc",
    "lint": "npm run type-check && npm run lint:format && npm run lint:fix"
  },
  "dependencies": {
    "react": "^18.0.0",
    "react-dom": "^18.0.0"
  },
  "devDependencies": {
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^5.30.5",
    "@typescript-eslint/parser": "^5.30.5",
    "@vitejs/plugin-react": "^1.3.0",
    "eslint": "^8.19.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.6.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.30.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "node-sass": "^7.0.1",
    "pre-commit": "^1.2.2",
    "prettier": "^2.7.1",
    "typescript": "^4.6.3",
    "vite": "^2.9.9"
  },
  "pre-commit": "lint"
}
